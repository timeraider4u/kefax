<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated kconfig -->
      <package
            uri="http://www.eclipse.org/MoDisco/KCONFIG/0.1.incubation/kconfig"
            class="org.eclipse.modisco.kconfig.KconfigPackage"
            genModel="model/kconfig.genmodel"/>
   </extension>

	<!-- so that model editors know they can open it -->
	<extension
		point="org.eclipse.core.contenttype.contentTypes">
		<content-type
			base-type="org.eclipse.emf.ecore.xmi"
			file-extensions="kconfigxmi"
			id="org.eclipse.gmt.modisco.kconfig.contenttype"
			name="%content-type.name"
			priority="normal">
		</content-type>
	</extension>
null
</plugin>
