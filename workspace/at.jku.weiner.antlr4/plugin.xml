<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
  <extension
        point="org.eclipse.ui.commands">
     <command
           defaultHandler="at.jku.weiner.antlr4.Antlr4Action"
           id="at.jku.weiner.antlr4.command"
           name="Antlr4 command">
     </command>
      <command
           defaultHandler="at.jku.weiner.antlr4.Antlr4PrintAction"
           id="at.jku.weiner.antlr4print.command"
           name="Antlr4 print command">
     </command>
  </extension>
  
  <extension
        point="org.eclipse.ui.menus">
     <menuContribution
           allPopups="false"
           locationURI="menu:org.eclipse.ui.main.menu">
        <menu
              id="at.jku.weiner.kefax.main.filemenu"
              label="KeFaX" />
     </menuContribution>
  </extension>
  <extension
        point="org.eclipse.ui.menus">
     <menuContribution
           allPopups="false"
           locationURI="menu:at.jku.weiner.kefax.main.filemenu">
           <command
                 commandId="at.jku.weiner.antlr4.command"
                 label="Antlr4"
                 style="push"
                 tooltip="Antlr4 test command">
           </command>
           <command
                 commandId="at.jku.weiner.antlr4print.command"
                 label="Antlr4 print"
                 style="push"
                 tooltip="Antlr4 print command">
           </command>
       </menuContribution>
     </extension>
</plugin>
