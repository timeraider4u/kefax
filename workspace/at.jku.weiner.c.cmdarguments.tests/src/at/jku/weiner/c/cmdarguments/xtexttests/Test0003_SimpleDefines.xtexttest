package at.jku.weiner.c.cmdarguments
language cmdArgs
// imports at.jku.weiner.c.common.common
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0003_SimpleDefines.cmd"
//----------------------------------------------------------------------
lexer-tokens
	ID WS SKW_COLON SKW_ASSIGN WS KW_DEFINE ID NEWLINE
	ID WS SKW_COLON SKW_ASSIGN WS KW_DEFINE ID NEWLINE
	ID WS SKW_COLON SKW_ASSIGN WS KW_DEFINE ID
//----------------------------------------------------------------------

(Model
	lines+=
	(CmdLine
		start++"foobar.o",
		arguments+=
		(Argument
			macro=
			(SimpleMacro
				name++"__FOO__BAR__"
			)
		)
	),
	(CmdLine
		start++"foobar.o",
		arguments+=
		(Argument
			macro=
			(SimpleMacro
				name++"FOO2"
			)
		)
	),
	(CmdLine
		start++"foobar.o",
		arguments+=
		(Argument
			macro=
			(SimpleMacro
				name++"_BAR2"
			)
		)
	)
)


