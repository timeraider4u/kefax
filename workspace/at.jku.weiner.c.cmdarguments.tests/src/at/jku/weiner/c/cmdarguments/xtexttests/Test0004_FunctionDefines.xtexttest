//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.c.cmdarguments
language cmdArgs
// imports at.jku.weiner.c.common.common
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0004_FunctionDefines.cmd"
//----------------------------------------------------------------------
lexer-tokens
	KW_DEFINE ID SKW_LEFTPAREN ID SKW_RIGHTPAREN SKW_ASSIGN ID SKW_LEFTPAREN ID SKW_RIGHTPAREN NEWLINE
	KW_DEFINE ID SKW_LEFTPAREN ID SKW_RIGHTPAREN SKW_ASSIGN ANY_OTHER ID
		// NEWLINE
		WS
		KW_DEFINE ID SKW_ASSIGN INT NEWLINE
	KW_DEFINE ID SKW_LEFTPAREN ID SKW_RIGHTPAREN SKW_ASSIGN ANY_OTHER ID NEWLINE
	KW_DEFINE ID SKW_LEFTPAREN ID SKW_COMMA ID SKW_RIGHTPAREN SKW_ASSIGN ID SKW_LEFTPAREN ID SKW_RIGHTPAREN ANY_OTHER +*2 ID NEWLINE
	
//----------------------------------------------------------------------

(Model
	lines+=
	(CmdLine
		arguments+=
		(Argument
			macro=
			(FunctionMacro
				name++"FOO",
				params+*"[X]",
				value++"BAR(X)"
			)
		)
	),
	(CmdLine
		arguments+=
		(Argument
			macro=
			(FunctionMacro
				name++"BAR",
				params+*"[x]",
				value++"#x"
			)
		),
		(Argument
			macro=
			(ObjectMacro
				name++"_BAR2",
				value++"5"
			)
		)
	),
	(CmdLine
		arguments+=
		(Argument
			macro=
			(FunctionMacro
				name++"STR",
				params+*"[s]",
				value++"#s"
			)
		)
	),
	(CmdLine
		arguments+=
		(Argument
			macro=
			(FunctionMacro
				name++"FOOBAR",
				params+*"[X, Y]",
				value++"FOO(X)##Y"
			)
		)
	)
)


