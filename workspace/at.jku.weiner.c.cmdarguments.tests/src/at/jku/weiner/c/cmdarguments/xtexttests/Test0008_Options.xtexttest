package at.jku.weiner.c.cmdarguments
language cmdArgs
// imports at.jku.weiner.c.common.common
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0008_Options.cmd"
//----------------------------------------------------------------------
lexer-tokens
	SKW_MINUS ID NEWLINE
	SKW_MINUS ID WS
		SKW_MINUS ID SKW_MINUS ID SKW_MINUS ID SKW_MINUS ID NEWLINE
	SKW_MINUS ID SKW_ASSIGN ID NEWLINE
	SKW_MINUS ID SKW_ASSIGN ID NEWLINE 
//----------------------------------------------------------------------

(Model
	lines+=
	(CmdLine
		arguments+=
		(Argument
			option++"Wundef"
			
		)
	),
	(CmdLine
		arguments+=
		(Argument
			option++"Wall"
			
		),
		(Argument
			option++"Werror-implicit-function-declaration"
			
		)
	),
	(CmdLine
		arguments+=
		(Argument
			option++"std=gnu89"
			
		)
	),
	(CmdLine
		arguments+=
		(Argument
			option++"mtune=generic"
			
		)
	)
)


