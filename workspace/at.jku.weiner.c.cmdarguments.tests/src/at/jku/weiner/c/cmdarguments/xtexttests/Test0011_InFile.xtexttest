package at.jku.weiner.c.cmdarguments
language cmdArgs
// imports at.jku.weiner.c.common.common
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0011_InFile.cmd"
//----------------------------------------------------------------------
lexer-tokens
	KW_DEFINE ID WS
		KW_OUTPUT WS ID ANY_OTHER ID WS
		ID ANY_OTHER ID
		NEWLINE
	ANY_OTHER +*2 ID ANY_OTHER ID SKW_MINUS ID ANY_OTHER ID WS	
		KW_DEFINE ID NEWLINE
	KW_DEFINE ID WS
		ID ANY_OTHER+*2 ID ANY_OTHER ID ANY_OTHER ID WS
		KW_DEFINE ID NEWLINE
//----------------------------------------------------------------------

(Model
	lines+=
	(CmdLine
