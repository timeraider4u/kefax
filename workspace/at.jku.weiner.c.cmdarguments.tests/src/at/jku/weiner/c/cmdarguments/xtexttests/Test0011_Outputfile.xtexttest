package at.jku.weiner.c.cmdarguments
language cmdArgs
// imports at.jku.weiner.c.common.common
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0011_Outputfile.cmd"
//----------------------------------------------------------------------
lexer-tokens
	ID WS SKW_COLON SKW_ASSIGN WS KW_OUTPUT WS STRING2 WS KW_DEFINE ID NEWLINE
	ID WS SKW_COLON SKW_ASSIGN WS KW_DEFINE ID WS
		// -o ./arch/file_arch.h
		KW_OUTPUT WS ID
		NEWLINE
//----------------------------------------------------------------------

(Model
	lines+=
	(CmdLine
		arguments+=
		(Argument
			out=
			(PathCmd
				path++"\\\"C:\\\\Program Files\\\\MyApp\\\\temp\\\\temp.c\\\""
			)
		),
		(Argument
			macro=
			(SimpleMacro
				name++"__TEMP__"
			)
		)
	),
	(CmdLine
		arguments+=
		(Argument
			macro=
			(SimpleMacro
				name++"FOO__"
			)
		),
		(Argument
			out=
			(PathCmd
				path++"./arch/file_arch.h"
			)
		)
	)
)


