package at.jku.weiner.c.cmdarguments
language cmdArgs
// imports at.jku.weiner.c.common.common
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0020_Variables.cmd"
//----------------------------------------------------------------------
lexer-tokens
	ID WS SKW_COLON SKW_ASSIGN WS SKW_MINUS ID NEWLINE
	ID WS SKW_COLON SKW_ASSIGN WS 
		KW_VAR NEWLINE
	KW_VAR WS SKW_COLON SKW_ASSIGN WS 
		SKW_MINUS ID SKW_ASSIGN ID NEWLINE
	KW_VAR WS SKW_COLON SKW_ASSIGN WS 
		KW_VAR NEWLINE 
//----------------------------------------------------------------------

(Model
	lines+=
	(CmdLine
		start++"foobar1.o",
		arguments+=
		(Argument
			option++"Wundef"
		)
	),
	(CmdLine
		start++"foobar2.o",
		arguments+=
		(Argument
			in++"foobar1.o",
			option isNull
		)
	),
	(CmdLine
		start++"my",
		arguments+=
		(Argument
			option++"std=gnu89"
			
		)
	),
	(CmdLine
		start++"x",
		arguments+=
		(Argument
			in++"my-own",
			option isNull
			
		)
	)
)


