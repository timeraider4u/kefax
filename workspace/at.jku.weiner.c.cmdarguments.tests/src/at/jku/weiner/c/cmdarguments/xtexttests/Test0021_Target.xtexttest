package at.jku.weiner.c.cmdarguments
language cmdArgs
// imports at.jku.weiner.c.common.common
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0021_Target.cmd"
//----------------------------------------------------------------------
lexer-tokens
	ID WS SKW_COLON SKW_ASSIGN WS ID WS SKW_MINUS ID WS ID NEWLINE
	NEWLINE
	ID SKW_COLON WS KW_VAR NEWLINE
	NEWLINE
	ID SKW_COLON WS NEWLINE
	NEWLINE
	ID SKW_COLON NEWLINE
		WS ID NEWLINE
		WS KW_VAR NEWLINE
//----------------------------------------------------------------------

(Model
	lines+=
	(CmdLine
		start++"foobar1.o",
		arguments+=
		(Argument
			option++"Wundef"
		)
	),
	(CmdLine
		start++"foobar2.o",
		arguments+=
		(Argument
			in++"foobar1.o",
			option isNull
		)
	),
	(CmdLine
		start++"my",
		arguments+=
		(Argument
			option++"std=gnu89"
			
		)
	),
	(CmdLine
		start++"x",
		arguments+=
		(Argument
			in++"my-own",
			option isNull
			
		)
	),
	(CmdLine
		start++"x",
		arguments+=
		(Argument
			in++"my own",
			option isNull
			
		)
	)
)


