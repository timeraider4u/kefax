emftest
package at.jku.weiner.c.modisco.discoverer.tests
default at.jku.weiner.c.common.common
imports at.jku.weiner.c.preprocess.preprocess as preprocess
imports at.jku.weiner.c.parser.parser as parser

class=EMFTest method=emfTestB()
options class=EMFTest method=getOptions() 

source file="res/Test0012_BatchMode"

(Model
	units+=
	// Predefined
	(TranslationUnit
		path++"platform:/plugin/at.jku.weiner.c.preprocess/res/predefined/gcc_4.8.4_default.h",
		preprocess=
		(preprocess::Preprocess
			
		)
	),
	// Simple.c
	(TranslationUnit
		preprocess=
		(preprocess::Preprocess
			group=
			(preprocess::GroupOpt
				lines+=
				(preprocess::PreprocessorDirectives
					directive=
					(preprocess::DefineObjectMacro
						id++"INT",
						string++"int"
					)
				),
				(preprocess::Code
					code++"unsigned INT i = 0;"
				)
			)
		),
		parser=
		(parser::Parser
			external+=
			(parser::ExternalDeclaration 
				declaration=
				(parser::Declaration
					specifiers=
					(parser::DeclarationSpecifiers
						declarationSpecifier+=
						(parser::TypeSpecifier
							name++"unsigned"
						),
						(parser::TypeSpecifier
							name++"int"
						)
					),
					initDeclaratorList+=
					(parser::InitDeclaratorList
						initDeclarator+=
						(parser::InitDeclarator
							declarator=
							(parser::Declarator
								declarator=
								(parser::DirectDeclarator
									id++"i"
								)
							),
							initializer=
							(parser::Initializer
								expr=
								(parser::PrimaryExpression
									const=
									(Constant2
										dec++"0"
									)
								) // end expression
							)
						)
					)
				)	
			)
		),
		path++"${{path}}/${{plugin_id}}/res/Test0012_BatchMode1/Simple.c"
	),
	// Another.c
	(TranslationUnit
		path++"${{path}}/${{plugin_id}}/res/Test0012_BatchMode2/Another.c",
		preprocess=
		(preprocess::Preprocess
			group=
			(preprocess::GroupOpt
				lines+=
				(preprocess::PreprocessorDirectives
					directive=
					(preprocess::DefineObjectMacro
						id++"ANOTHER",
						string++"1"
					)
				),
				(preprocess::Code
					code++"int j = ANOTHER;"
				)
			)
		),
		parser=
		(parser::Parser
			external+=
			(parser::ExternalDeclaration 
				declaration=
				(parser::Declaration
					specifiers=
					(parser::DeclarationSpecifiers
						declarationSpecifier+=
						(parser::TypeSpecifier
							name++"int"
						)
					),
					initDeclaratorList+=
					(parser::InitDeclaratorList
						initDeclarator+=
						(parser::InitDeclarator
							declarator=
							(parser::Declarator
								declarator=
								(parser::DirectDeclarator
									id++"j"
								)
							),
							initializer=
							(parser::Initializer
								expr=
								(parser::PrimaryExpression
									const=
									(Constant2
										dec++"1"
									)
								) // end expression
							)
						)
					)
				)	
			)
		)
	)
)

before class=at.jku.weiner.c.modisco.discoverer.tests.EMFTest method=test0012()
