										(TypeSpecifier
											specifier=
											(StructOrUnionSpecifier
												type=
												(StructOrUnion
													name++"struct"
												),
												id++"temp"
											)
										)
									),
									structDeclarationList=
									(StructDeclaratorList
										structDeclarator+=
										(StructDeclarator
											declarator=
											(Declarator
												pointer=
												(Pointer
													star+*"[*]"
												),
												declarator=
												(DirectDeclarator
													id++"in1"
												)
											)
										)
									)
								),
								(StructDeclaration
									list=
									(SpecifierQualifierList
										typeSpecifier+=
										(TypeSpecifier
											specifier=
										
											(StructOrUnionSpecifier
												type=
												(StructOrUnion
													name++"struct"
												),
												id++"temp"
											)
										//	typeSpecifier+=
										//	(TypeSpecifier
										//		name++"int"
										//	)
										)
									),
									structDeclarationList=
									(StructDeclaratorList
										structDeclarator+=
										(StructDeclarator
											declarator=
											(Declarator
												declarator=
												(DirectDeclarator
													id++"in2"
												)
											)
										)
									)
								),
								(StructDeclaration
									list=
									(SpecifierQualifierList
										typeSpecifier+=
										(TypeSpecifier
											specifier=
											(StructOrUnionSpecifier
												type=
												(StructOrUnion
													name++"union"
												),
												structDeclList=
												// inner thing of union
												(StructDeclarationList
													structDeclaration+=
													(StructDeclaration
