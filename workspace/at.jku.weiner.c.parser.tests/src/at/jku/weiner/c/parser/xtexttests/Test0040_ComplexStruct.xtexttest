	//(common::TranslationUnit 
	(Parser
		external+=
		// struct temp { ... };
		(ExternalDeclaration 
			declaration=
			(Declaration
				specifiers=
				(DeclarationSpecifiers
					declarationSpecifier+=
					(TypeSpecifier
						specifier=
						(StructOrUnionSpecifier
							type=
							(StructOrUnion
								name++"struct"
							),
							id++"temp",
							structDeclList=
							(StructDeclarationList
								structDeclaration+=
								(StructDeclaration
