//----------------------------------------------------------------------
package at.jku.weiner.c.preprocess
language preprocess
imports at.jku.weiner.c.common.common
//----------------------------------------------------------------------
split-lexer=false
source file="res/Test0003_Error.h"
//----------------------------------------------------------------------
lexer-tokens
	HASH WARNING WHITESPACE STRING_LITERAL NEWLINE
	HASH WARNING NEWLINE
	HASH ERROR WHITESPACE STRING_LITERAL NEWLINE
	HASH ERROR NEWLINE
//----------------------------------------------------------------------
(common::Model
	units+=
	(common::TranslationUnit
	preprocess=(Preprocess
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(WarningDirective
					msg++"\\\"warning message!\\\""
				)
			),
			(PreprocessorDirectives
				directive=
				(WarningDirective
					msg isNull
				)
			),
			(PreprocessorDirectives
				directive=
				(ErrorDirective
					msg++"\\\"error message!\\\""
				)
			),
			(PreprocessorDirectives
				directive=
				(ErrorDirective
					msg isNull
				)
			)
			
		)
	)
)
)
//----------------------------------------------------------------------
output file="bin/Test0003_Error.h.i"
expected file="expected/Test0003_Error.h"
//replacePatterns file="res/Patterns.txt"
