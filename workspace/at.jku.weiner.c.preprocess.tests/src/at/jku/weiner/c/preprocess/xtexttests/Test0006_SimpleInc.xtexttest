package at.jku.weiner.c.preprocess
language preprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0006_SimpleInc.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH INCLUDE WS STRING_LITERAL NEWLINE
	HASH INCLUDE WS STRING_LITERAL NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0000_Empty.c\\\""
				)
			),
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0002_Code.c\\\""
				)
			)
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0006_SimpleInc.c.i"
expected file="expected/Test0006_SimpleInc.c"
