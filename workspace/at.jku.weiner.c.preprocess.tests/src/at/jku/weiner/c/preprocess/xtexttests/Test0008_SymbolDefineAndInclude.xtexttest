package at.jku.weiner.c.preprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0008_SymbolDefineAndInclude.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH DEFINE WS ID WS STRING_LITERAL NEWLINE
	HASH INCLUDE WS STRING_LITERAL NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(DefineDirective
					id++"abc",
					string++"\\\"Test0002_Code.c\\\""
				)
			),
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0007_SymbolInInclude.h\\\""
				)
			)
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0008_SymbolDefineAndInclude.c.i"
expected file="expected/Test0008_SymboleDefineAndInclude.c"
//replacePatterns file="res/Patterns.txt"
