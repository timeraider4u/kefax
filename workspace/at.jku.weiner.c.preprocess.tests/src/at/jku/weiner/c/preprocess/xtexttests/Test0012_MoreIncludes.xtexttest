package at.jku.weiner.c.preprocess
language preprocess
//----------------------------------------------------------------------
split-lexer=false
source file="res/Test0012_MoreIncludes.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH WS INCLUDE WS STRING_LITERAL NEWLINE
	NEWLINE
	
	LINE_COMMENT NEWLINE
	LINE_COMMENT NEWLINE
	
	HASH WS LINEBREAK WS INCLUDE LINEBREAK WS STRING_LITERAL NEWLINE
	HASH INCLUDE WS STRING_LITERAL NEWLINE
	NEWLINE
	BLOCK_COMMENT NEWLINE
	NEWLINE
	HASH INCLUDE WS+*2 STRING_LITERAL NEWLINE
	NEWLINE
	HASH WS INCLUDE WS+*4 STRING_LITERAL NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0000_Empty.c\\\""
				)
			),
			(NewLineLine),
			(NewLineLine),
			(NewLineLine),
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0000_Empty.c\\\""
				)
			),
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0000_Empty.c\\\""
				)
			),
			(NewLineLine),
			(NewLineLine),
			(NewLineLine),
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0000_Empty.c\\\""
				)
			),
			(NewLineLine),
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0002_Code.c\\\""
				)
			)
			
		)
	)
)
