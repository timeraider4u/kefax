//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.c.preprocess
language preprocess
//----------------------------------------------------------------------
split-lexer=false
source file="res/Test0013_RelativeInclude.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH INCLUDE WS STRING_LITERAL NEWLINE
	NEWLINE
	ID WS ID SKW_LEFTPAREN ID SKW_RIGHTPAREN WS SKW_LEFTBRACE NEWLINE
	WS ID SKW_LEFTPAREN STRING_LITERAL SKW_RIGHTPAREN SKW_SEMI NEWLINE
	WS ID WS DECIMAL_LITERAL SKW_SEMI NEWLINE
	SKW_RIGHTBRACE NEWLINE
	NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"../myinclude/include/printf.h\\\""
				)
			),
			(NewLineLine),
			(Code
				code++"int main(void) {"
			),
			(Code
				code++"	printf(\\\"Hello World!\\\\n\\\");"
			),
			(Code
				code++"	return 0;"
			),
			(Code
				code++"}"
			),
			
			(NewLineLine
				
			)
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0013_RelativeInclude.c.i"
expected file="expected/Test0013_RelativeInclude.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
