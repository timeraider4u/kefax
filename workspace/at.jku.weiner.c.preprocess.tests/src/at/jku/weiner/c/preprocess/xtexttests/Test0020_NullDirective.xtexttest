package at.jku.weiner.c.preprocess
language preprocess
//----------------------------------------------------------------------
split-lexer = true
source file="res/Test0020_NullDirective.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(NullDirective)
			)
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0020_NullDirective.c.i"
expected file="expected/Test0020_NullDirective.c"
