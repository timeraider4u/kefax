package at.jku.weiner.c.preprocess
language preprocess
imports at.jku.weiner.c.common.common
//----------------------------------------------------------------------
split-lexer=false
source file="res/Test0028_SplittedDefine.c"
//----------------------------------------------------------------------
lexer-tokens
	LINE_COMMENT NEWLINE
	LINE_COMMENT NEWLINE
	LINE_COMMENT NEWLINE
	LINE_COMMENT NEWLINE
	LINE_COMMENT NEWLINE
	LINE_COMMENT NEWLINE
	LINE_COMMENT NEWLINE
	LINE_COMMENT NEWLINE
	NEWLINE
	LINE_COMMENT NEWLINE
	BLOCK_COMMENT WS HASH WS BLOCK_COMMENT WS DEFINE LINEBREAK WS WS ID WS LINEBREAK WS WS DECIMAL_LITERAL NEWLINE
	NEWLINE
	
	ID WS ID WS SKW_ASSIGN WS ID SKW_SEMI NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(NewLineLine),
			(NewLineLine),
			(NewLineLine),
			(NewLineLine),
			(NewLineLine),
			(NewLineLine),
			(NewLineLine),
			(NewLineLine),
			(NewLineLine),
			(NewLineLine),
			(PreprocessorDirectives
				directive=
				(DefineDirective
					id++"FOO",
					string++"1024"
				)
			),
			(NewLineLine),
			(Code
				code++"int foo = FOO;"
			)
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0028_SplittedDefine.c.i"
expected file="expected/Test0028_SplittedDefine.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
