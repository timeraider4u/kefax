//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.c.preprocess
language preprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0034_Stringification1.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH DEFINE WS ID SKW_LEFTPAREN ID SKW_RIGHTPAREN WS ID SKW_LEFTPAREN
		HASH ID WS
		STRING_LITERAL
		SKW_COMMA WS ID SKW_RIGHTPAREN 
		NEWLINE
	ID SKW_LEFTPAREN DECIMAL_LITERAL SKW_RIGHTPAREN SKW_SEMI NEWLINE
	ID WS ID WS SKW_ASSIGN WS DECIMAL_LITERAL SKW_SEMI NEWLINE
	ID SKW_LEFTPAREN ID SKW_RIGHTPAREN SKW_SEMI NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(DefineFunctionLikeMacro
					id++"DEBUG",
					list=
					(IdentifierList
						id+*"[X]"
					),
					string++"printf(#X \\\"=%d\\\\n\\\", X)"
				)
			),
			(Code
				code++"DEBUG(5);"
			),
			(Code
				code++"int x = 5;"
			),
			(Code
				code++"DEBUG(x);"
			)
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0034Test0034_Stringification1.c.i"
expected file="expected/Test0034_Stringification1.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
