//----------------------------------------------------------------------
package at.jku.weiner.c.preprocess
language preprocess
imports at.jku.weiner.c.common.common
//----------------------------------------------------------------------
split-lexer=false
source file="res/Test0037_Concatenation.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH DEFINE WS ID SKW_LEFTPAREN ID SKW_RIGHTPAREN WS ID WS HASH HASH WS ID NEWLINE
	ID SKW_LEFTPAREN ID SKW_RIGHTPAREN NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(DefineFunctionLikeMacro
					id++"FOO",
					list=
					(IdentifierList
						id+*"[x]"
					),
					string++"x ## BAR"
				)
			),
			(Code
				code++"FOO(foo)"
			)
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0037_Concatenation.c.i"
expected file="expected/Test0037_Concatenation.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
