package at.jku.weiner.c.preprocess
language preprocess
imports at.jku.weiner.c.common.common
//----------------------------------------------------------------------
split-lexer=false
source file="res/Test0054_IfDefined.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH DEFINE WHITESPACE ID WHITESPACE DECIMAL_LITERAL NEWLINE
	HASH IF WHITESPACE DEFINED WHITESPACE SKW_LEFTPAREN ID SKW_RIGHTPAREN WHITESPACE 
		SKW_OROR WHITESPACE DEFINED WHITESPACE SKW_LEFTPAREN ID SKW_RIGHTPAREN NEWLINE
		WHITESPACE ID WHITESPACE ID SKW_LEFTPAREN SKW_RIGHTPAREN SKW_SEMI NEWLINE
	HASH ENDIF NEWLINE
//----------------------------------------------------------------------
//(common::Model
	//units+=
	//(common::TranslationUnit
	(Preprocess
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(DefineObjectMacro
					id++"BAR",
					string++"0"
				)
			),
			(PreprocessorDirectives
				directive=
				(ConditionalDirective
					conditional=
					(IfConditional
						expression=
						(common::LogicalOrExpression
							left=
							(PrimaryExpression
								defined?=true,
								id++"FOO"
							),
							right=
							(PrimaryExpression
								defined?=true,
								id++"BAR"
							)
						),
						group=
						(GroupOpt
							lines+=
							(Code
								code++"	void foo_bar();"
							)
						)
					)
				)
			)
		)
	)
//)
//)
//----------------------------------------------------------------------
output file="bin/Test0054_IfDefined.c.i"
expected file="expected/Test0054_IfDefined.c"
//replacePatterns file="res/Patterns.txt"
