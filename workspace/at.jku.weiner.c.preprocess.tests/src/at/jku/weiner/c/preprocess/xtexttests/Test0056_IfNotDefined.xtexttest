lexer-tokens
	//HASH DEFINE WS ID WS DECIMAL_LITERAL NEWLINE
	HASH IF WS SKW_NOT DEFINED WS ID NEWLINE
		WS ID WS ID SKW_LEFTPAREN SKW_RIGHTPAREN SKW_SEMI NEWLINE
	HASH ENDIF NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
