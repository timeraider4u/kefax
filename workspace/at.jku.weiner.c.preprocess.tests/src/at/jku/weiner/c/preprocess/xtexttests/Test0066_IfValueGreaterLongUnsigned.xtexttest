package at.jku.weiner.c.preprocess
language preprocess
imports at.jku.weiner.c.common.common
//----------------------------------------------------------------------
split-lexer=false
source file="res/Test0066_IfValueGreaterLongUnsigned.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH DEFINE WHITESPACE ID WHITESPACE DECIMAL_LITERAL NEWLINE
	HASH IF WHITESPACE SKW_LEFTPAREN DEFINED WHITESPACE ID WHITESPACE SKW_ANDAND
			WHITESPACE ID WHITESPACE SKW_GREATEREQUAL
			WHITESPACE DECIMAL_LITERAL SKW_RIGHTPAREN NEWLINE
		WHITESPACE ID WHITESPACE ID SKW_LEFTPAREN SKW_RIGHTPAREN SKW_SEMI NEWLINE
	HASH ENDIF NEWLINE
//----------------------------------------------------------------------
//(common::Model
	//units+=
	//(common::TranslationUnit
	(Preprocess
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(DefineObjectMacro
					ident++"__STDC_VERSION__",
					string++"201300UL"
				)
			),
			(PreprocessorDirectives
				directive=
				(ConditionalDirective
					conditional=
					(IfConditional
						expression=
						(PrimaryExpression
							parentheses?=true,
							//ident++"__STDC_VERSION__"
							expr=
							(common::LogicalAndExpression
								left=
								(PrimaryExpression
									defined?=true,
									ident++"__STDC_VERSION__"
								),
								right=
								(common::RelationalExpression
									op++">=",
									left=
									(PrimaryExpression
										ident++"__STDC_VERSION__"
									),
									right=
									(PrimaryExpression
										const++"201112L"
									)
								)
							)
						),
						group=
						(GroupOpt
							lines+=
							(Code
								code++"	void c11_extension();"
							)
						)
					)
				)
			)
		)
	)
//)
//)
