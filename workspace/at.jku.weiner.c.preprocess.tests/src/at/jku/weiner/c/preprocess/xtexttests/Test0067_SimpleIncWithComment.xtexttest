package at.jku.weiner.c.preprocess
language preprocess
imports at.jku.weiner.c.common.common
//----------------------------------------------------------------------
split-lexer=false
source file="res/Test0067_SimpleIncWithComment.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH INCLUDE WHITESPACE STRING_LITERAL WHITESPACE BLOCK_COMMENT NEWLINE
	HASH INCLUDE WHITESPACE STRING_LITERAL LINE_COMMENT NEWLINE
//----------------------------------------------------------------------
(common::Model
	units+=
	(common::TranslationUnit
	preprocess=(Preprocess
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0000_Empty.c\\\""
				)
			),
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0002b_Code.c\\\""
				)
			)
			
		)
	)
)
)
//----------------------------------------------------------------------
output file="bin/Test0067_SimpleIncWithComment.c.i"
expected file="expected/Test0067_SimpleIncWithComment.c"
