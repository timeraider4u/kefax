package at.jku.weiner.c.preprocess
language preprocess
imports at.jku.weiner.c.common.common
//----------------------------------------------------------------------
split-lexer=false
source file="res/Test0085_If.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH DEFINE WHITESPACE ID WHITESPACE DECIMAL_LITERAL NEWLINE
	HASH DEFINE WHITESPACE ID WHITESPACE ID NEWLINE
	HASH IF WHITESPACE ID WHITESPACE SKW_EQUAL WHITESPACE ID NEWLINE
		WHITESPACE ID WHITESPACE ID SKW_LEFTPAREN SKW_RIGHTPAREN SKW_SEMI NEWLINE
	HASH ENDIF NEWLINE
//----------------------------------------------------------------------
//(common::Model
	//units+=
	//(common::TranslationUnit
	(Preprocess
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(DefineObjectMacro
					id++"__FOO",
					string++"1234"
				)
			),
			(PreprocessorDirectives
				directive=
				(DefineObjectMacro
					id++"__BAR",
					string++"__FOO"
				)
			),
			(PreprocessorDirectives
				directive=
				(ConditionalDirective
					conditional=
					(IfConditional
						expression=
						(common::EqualityExpression
							op++"==",
							left=
							(PrimaryExpression
								id++"__FOO"
							),
							right=
							(PrimaryExpression
								id++"__BAR"
							)
						),
						group=
						(GroupOpt
							lines+=
							(Code
								code++"	void doSomething();"
							)
						)
					)
				)
			)
		)
	)
//)
//)
//----------------------------------------------------------------------
output file="bin/Test0085_If.c.i"
expected file="expected/Test0085_If.c"
//replacePatterns file="res/Patterns.txt"
