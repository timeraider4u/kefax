	(Preprocess
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(DefineObjectMacro
					id++"FOO",
					string++"x"
				)
			),
			(PreprocessorDirectives
				directive=
				(DefineFunctionLikeMacro
					id++"BAR",
					list=
					(IdentifierList
						id+*"[x]"
					),
					string++"#x"
				)
			),
			(Code
				code++"foo"
			),
			(Code
				code++"foo(y)"
			),
			(Code
				code++"bar"
			),
			(Code
				code++"bar(y)"
			),
			(Code
				code++"Foo"
			),
			(Code
				code++"Foo(y)"
			),
			(Code
				code++"Bar"
			),
			(Code
				code++"Bar(y)"
			),
			(Code
				code++"FOO"
			),
			(Code
				code++"FOO(y)"
			),
			(Code
				code++"BAR"
			),
			(Code
				code++"BAR(y)"
			),
			(NewLineLine),
			(Code
				code++"BAR FOO"
			),
			(Code
				code++"BAR(FOO(y))"
			),
			(Code
				code++"BAR(Foo)"
			),
			(Code
				code++"BAR(foo)"
			),
			(NewLineLine),
			(PreprocessorDirectives
				directive=
				(DefineFunctionLikeMacro
					id++"BAZ",
					list=
					(IdentifierList
						id+*"[z]"
					),
					string++"FOO foo z Foo #z"
				)
			),
			(Code
				code++"baz(y)"
			),
			(Code
				code++"BAZ(Foo)"
			),
			(Code
				code++"BAZ(foo)"
			),
			(Code
				code++"BAZ(FOO)"
			)
		)
	)
