package at.jku.weiner.c
language c
//----------------------------------------------------------------------
source file="res/Test0016_PointerVariable.c"
//----------------------------------------------------------------------
lexer-tokens
KW_FLOAT WHITESPACE STAR WHITESPACE KW_CONST WHITESPACE ID SEMI NEWLINE
KW_DOUBLE WHITESPACE STAR WHITESPACE STAR WHITESPACE ID SEMI NEWLINE
//----------------------------------------------------------------------
(Model unit+=
	(TranslationUnit 
		external+=
		(ExternalDeclaration 
			declaration=
			(Declaration
				specifiers=
				(DeclarationSpecifiers
					declarationSpecifier+=
					(TypeSpecifier
						name++"float"
					)
				),
				initDeclaratorList+=
				(InitDeclaratorList
					initDeclarator+=
					(InitDeclarator
						declarator=
						(Declarator
							declarator=
							(DirectDeclarator
								id++"a"
							),
							pointer=
							(Pointer
								star+*"[*]",
								typeQualifierList+=
								(TypeQualifierList
									typeQualifier+=
									(TypeQualifier
										type++"const"
									)
								)
							)
						)
					)
				),
				semi++";"
			)
		),
		(ExternalDeclaration
			declaration=
			(Declaration
				specifiers=
				(DeclarationSpecifiers
					declarationSpecifier+=
					(TypeSpecifier
						name++"double"
					)
				)
				,
				initDeclaratorList+=
				(InitDeclaratorList
					initDeclarator+=
					(InitDeclarator
						declarator=
						(Declarator
							declarator=
							(DirectDeclarator
								id++"b"
							),
							pointer=
							(Pointer
								star+*"[*, *]"
							)
						)
					)
				),
				semi++";"
			)
			
		)
	)
)
