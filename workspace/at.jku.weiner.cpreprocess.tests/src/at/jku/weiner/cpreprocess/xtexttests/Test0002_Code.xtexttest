package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0002_Code.c"
//----------------------------------------------------------------------
lexer-tokens
	ID WS ID LPAREN ID RPAREN WS SPECIAL NEWLINE
	WS ID WS SPECIAL +* 2 NEWLINE
	SPECIAL NEWLINE
	NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(Code
				code++"int main(void) {"
			),
			(Code
				code++"	return 0;"
			),
			(Code
				code++"}"
			),
			
			(NewLineLine
				
			)
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0002_Code.c.i"
expected file="expected/Test0002_Code.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
