package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0003_Error.h"
//----------------------------------------------------------------------
lexer-tokens
	HASH ERROR WS SPECIAL ERROR WS ID SPECIAL +*2 NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		lines+=
		(PreprocessorDirectives
			directive=
			(ErrorDirective
				msg++"\\\"error message!\\\""
			)
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0003_Error.h.i"
expected file="expected/Test0003_Error.h"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
