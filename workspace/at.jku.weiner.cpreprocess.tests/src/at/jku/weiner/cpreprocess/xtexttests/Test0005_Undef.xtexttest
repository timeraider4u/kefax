			code++"int a = 2.5;"
		),
		(PreprocessorDirectives
			directive=
			(UnDefineDirective
				id++"int"
			)
		),
		(Code
			code++"int b = 2;"
		),
		(PreprocessorDirectives
			directive=
			(DefineDirective
				id++"int",
				string++"long"
			)
		),
		(PreprocessorDirectives
			directive=
			(UnDefineDirective
				id++"int"
			)
		),
		(Code
			code++"int c = 2;"
		),
		(PreprocessorDirectives
			directive=
			(UnDefineDirective
				id++"int"
			)
		),
		(Code
			code++"int d = 4;"
		)
	)
)
