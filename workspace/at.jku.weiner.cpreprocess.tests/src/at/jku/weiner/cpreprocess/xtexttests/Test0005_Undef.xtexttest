lexer-tokens
	MYCODE NEWLINE
	DEFINE ID MYCODE NEWLINE
	MYCODE NEWLINE
	UNDEF ID NEWLINE
	MYCODE NEWLINE
	DEFINE ID MYCODE NEWLINE
