//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0005_Undef.c"
//----------------------------------------------------------------------
lexer-tokens
	SPECIAL +*2 WS ID COMMA WS ID WS ID NEWLINE
	HASH DEFINE WS ID WS WS ID NEWLINE
	ID WS ID WS SPECIAL WS SPECIAL +*4 NEWLINE
	HASH UNDEF WS ID NEWLINE
	ID WS ID WS SPECIAL WS SPECIAL +*2 NEWLINE
	HASH DEFINE WS ID WS WS ID NEWLINE
	HASH UNDEF WS ID NEWLINE
	ID WS ID WS SPECIAL WS SPECIAL +*2 NEWLINE
	HASH UNDEF WS ID NEWLINE
	ID WS ID WS SPECIAL WS SPECIAL +*2 NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(Code
				code++"// nasty, but valid"
			),
			(PreprocessorDirectives
				directive=
				(DefineDirective
					id++"int",
					string++"double"
				)
			),
			(Code
				code++"int a = 2.5;"
			),
			(PreprocessorDirectives
				directive=
				(UnDefineDirective
					id++"int"
				)
			),
			(Code
				code++"int b = 2;"
			),
			(PreprocessorDirectives
				directive=
				(DefineDirective
					id++"int",
					string++"long"
				)
			),
			(PreprocessorDirectives
				directive=
				(UnDefineDirective
					id++"int"
				)
			),
			(Code
				code++"int c = 2;"
			),
			(PreprocessorDirectives
				directive=
				(UnDefineDirective
					id++"int"
				)
			),
			(Code
				code++"int d = 4;"
			)
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0005_Undef.c.i"
expected file="expected/Test0005_Undef.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
