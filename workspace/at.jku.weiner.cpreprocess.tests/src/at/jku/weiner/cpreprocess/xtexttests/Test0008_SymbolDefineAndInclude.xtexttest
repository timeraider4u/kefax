package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0008_SymbolDefineAndInclude.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH DEFINE WS ID WS SPECIAL ID SPECIAL ID SPECIAL ID SPECIAL NEWLINE
	HASH INCLUDE WS SPECIAL ID SPECIAL ID SPECIAL ID SPECIAL NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		lines+=
		(PreprocessorDirectives
			directive=
			(DefineDirective
				id++"abc",
				string++"\\\"Test0002_Code.c\\\""
			)
		),
		(PreprocessorDirectives
			directive=
			(IncludeDirective
				string++"\\\"Test0007_SymbolInInclude.h\\\""
			)
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0008_SymbolDefineAndInclude.c.i"
expected file="expected/Test0008_SymboleDefineAndInclude.c"
