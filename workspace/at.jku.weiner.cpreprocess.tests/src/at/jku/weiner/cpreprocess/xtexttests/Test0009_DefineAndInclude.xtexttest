//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0009_DefineAndInclude.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH DEFINE WS ID WS ID NEWLINE
	HASH DEFINE WS ID WS ID NEWLINE
	HASH INCLUDE WS STRING_LITERAL NEWLINE
	NEWLINE
	ID SKW_SEMI NEWLINE
	NEWLINE
	ID WS ID SKW_LEFTPAREN ID SKW_RIGHTPAREN WS SKW_LEFTBRACE NEWLINE
	WS ID WS ID WS SKW_MINUS WS DECIMAL_LITERAL WS SKW_STAR WS ID SKW_SEMI NEWLINE
	SKW_RIGHTBRACE NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(DefineDirective
					id++"INT",
					string++"int"
				)
			),
			(PreprocessorDirectives
				directive=
				(DefineDirective
					id++"MAIN",
					string++"main"
				)
			),
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0009_DefineAndInclude.h\\\""
				)
			),
			(NewLineLine),
			(Code
				code++"DECLARATION;"
			),
			(NewLineLine),
			(Code
				code++"INT MAIN(void) {"
			),
			(Code
				code++"	return x - 2 * Y;"
			),
			(Code
				code++"}"
			)
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0009_DefineAndInclude.c.i"
expected file="expected/Test0009_DefineAndInclude.c"
//replacePatterns file="res/Patterns.txt"
