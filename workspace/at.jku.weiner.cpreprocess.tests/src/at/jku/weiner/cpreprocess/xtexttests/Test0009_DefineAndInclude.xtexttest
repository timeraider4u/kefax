//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0009_DefineAndInclude.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH DEFINE WS ID WS ID NEWLINE
	HASH DEFINE WS ID WS ID NEWLINE
	HASH INCLUDE WS SPECIAL ID SPECIAL ID SPECIAL NEWLINE
	NEWLINE
	ID SPECIAL NEWLINE
	NEWLINE
	ID WS ID SPECIAL ID SPECIAL WS SPECIAL NEWLINE
	WS ID WS ID WS SPECIAL WS SPECIAL WS SPECIAL WS ID SPECIAL NEWLINE
	SPECIAL NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		lines+=
		(PreprocessorDirectives
			directive=
			(DefineDirective
				id++"INT",
				string++"int"
			)
		),
		(PreprocessorDirectives
			directive=
			(DefineDirective
				id++"MAIN",
				string++"main"
			)
		),
		(PreprocessorDirectives
			directive=
			(IncludeDirective
				string++"\\\"Test0009_DefineAndInclude.h\\\""
			)
		),
		(NewLineLine),
		(Code
			code++"DECLARATION;"
		),
		(NewLineLine),
		(Code
			code++"INT MAIN(void) {"
		),
		(Code
			code++"	return x - 2 * Y;"
		),
		(Code
			code++"}"
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0009_DefineAndInclude.c.i"
expected file="expected/Test0009_DefineAndInclude.c"
//replacePatterns file="res/Patterns.txt"
