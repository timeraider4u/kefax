package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
source file="res/Test0009_DefineAndInclude.h"
//----------------------------------------------------------------------
lexer-tokens
	DEFINE ID MYCODE NEWLINE
	DEFINE ID MYCODE NEWLINE
	DEFINE ID MYCODE NEWLINE
	DEFINE ID MYCODE NEWLINE
	MYCODE NEWLINE
	MYCODE NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		lines+=
		(PreprocessorDirectives
			directive=
			(DefineDirective
				id++"X",
				string++"x"
			)
		),
		(PreprocessorDirectives
			directive=
			(DefineDirective
				id++"Y",
				string++"y"
			)
		),
		(PreprocessorDirectives
			directive=
			(DefineDirective
				id++"DECLARATION",
				string++"INT X = 4"
			)
		),
		(PreprocessorDirectives
			directive=
			(DefineDirective
				id++"Z",
				string++"1 + 1"
			)
		),
		(Code
			code++"// declarations"
		),
		(Code
			code++"int Y = Z;"
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0009_DefineAndInclude.h.i"
expected file="expected/Test0009_DefineAndInclude.h"
