//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0010_UglyInclude.c"
//----------------------------------------------------------------------
lexer-tokens
	ID WS ID SPECIAL ID SPECIAL WS SPECIAL NEWLINE
	WS ID WS SPECIAL SPECIAL WS SPECIAL WS SPECIAL +*2 WS SPECIAL NEWLINE
	WS WS HASH INCLUDE WS SPECIAL ID SPECIAL ID SPECIAL NEWLINE
	WS SPECIAL NEWLINE
	WS ID WS SPECIAL +*3 NEWLINE
	SPECIAL NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(Code
				code++"int main(void) {"
			),
			(Code
				code++"	if (1 > 0) {"
			),
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0010_UglyInclude.h\\\""
				)
			),
			(Code
				code++"	}"
			),
			(Code
				code++"	return -1;"
			),
			(Code
				code++"}"
			)
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0010_UglyInclude.c.i"
expected file="expected/Test0010_UglyInclude.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
