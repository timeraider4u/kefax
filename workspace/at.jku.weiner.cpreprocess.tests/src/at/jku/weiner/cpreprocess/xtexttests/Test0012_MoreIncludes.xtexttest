package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0012_MoreIncludes.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH WS INCLUDE WS STRING_LITERAL NEWLINE
	NEWLINE
	//SPECIAL SPECIAL WS INCLUDE WS SPECIAL ID SPECIAL ID SPECIAL NEWLINE
	WS NEWLINE
	//SPECIAL SPECIAL WS HASH INCLUDE WS SPECIAL ID SPECIAL ID SPECIAL NEWLINE
	WS NEWLINE
	HASH WS+*3 INCLUDE WS+*2 STRING_LITERAL NEWLINE
	HASH INCLUDE WS STRING_LITERAL NEWLINE
	NEWLINE
	//SPECIAL SPECIAL HASH INCLUDE WS SPECIAL ID SPECIAL ID SPECIAL SPECIAL SPECIAL NEWLINE
	WS NEWLINE
	NEWLINE
	HASH INCLUDE WS+*2 STRING_LITERAL NEWLINE
	NEWLINE
	HASH WS INCLUDE WS+*4 STRING_LITERAL NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0000_Empty.c\\\""
				)
			),
			(NewLineLine),
			(Code
				code++"// include \\\"Test0000_Empty.c\\\""
			),
			(Code
				code++"// #include \\\"Test0000_Empty.c\\\""
			),
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0000_Empty.c\\\""
				)
			),
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0000_Empty.c\\\""
				)
			),
			(NewLineLine),
			(Code
				code++"/*#include \\\"Test0000_Empty.c\\\"*/"
			),
			(NewLineLine),
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0000_Empty.c\\\""
				)
			),
			(NewLineLine),
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"\\\"Test0002_Code.c\\\""
				)
			)
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0012_MoreIncludes.c.i"
expected file="expected/Test0012_MoreIncludes.c"
