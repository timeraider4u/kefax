//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0013_RelativeInclude.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH INCLUDE WS SPECIAL+*4 ID SPECIAL INCLUDE SPECIAL ID SPECIAL ID SPECIAL NEWLINE
	NEWLINE
	ID WS ID SPECIAL ID SPECIAL WS SPECIAL NEWLINE
	WS ID SPECIAL+*2 ID WS ID SPECIAL +*2 ID SPECIAL +*3 NEWLINE
	WS ID WS SPECIAL +*2 NEWLINE
	SPECIAL NEWLINE
	NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		lines+=
		(PreprocessorDirectives
			directive=
			(IncludeDirective
				string++"\\\"../myinclude/include/printf.h\\\""
			)
		),
		(NewLineLine),
		(Code
			code++"int main(void) {"
		),
		(Code
			code++"	printf(\\\"Hello World!\\\\n\\\");"
		),
		(Code
			code++"	return 0;"
		),
		(Code
			code++"}"
		),
		
		(NewLineLine
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0013_RelativeInclude.c.i"
expected file="expected/Test0013_RelativeInclude.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
