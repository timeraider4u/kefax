package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="myinclude/include/printf.h"
//----------------------------------------------------------------------
lexer-tokens
	SPECIAL +*2 WS ID SPECIAL ID NEWLINE
	ID WS ID SPECIAL ID WS SPECIAL+*2 WS ID SPECIAL+*2 NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		lines+=
		(Code
			code++"// printf.h"
		),
		(Code
			code++"void printf(char ** argv);"
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0013_RelativeInclude.h.i"
expected file isSameAsInputFile
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
