//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0014_AbsoluteInclude.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH INCLUDE WS SPECIAL INCLUDE SPECIAL ID SPECIAL ID SPECIAL NEWLINE
	NEWLINE
	ID WS ID SPECIAL ID SPECIAL WS SPECIAL NEWLINE
	WS ID SPECIAL+*2 ID WS ID SPECIAL +*2 ID SPECIAL +*3 NEWLINE
	WS ID WS SPECIAL +*2 NEWLINE
	SPECIAL NEWLINE
	NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(IncludeDirective
					string++"<include/printf.h>"
				)
			),
			(NewLineLine),
			(Code
				code++"int main(void) {"
			),
			(Code
				code++"	printf(\\\"Hello World!\\\\n\\\");"
			),
			(Code
				code++"	return 0;"
			),
			(Code
				code++"}"
			),
			
			(NewLineLine
				
			)
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0014_AbsoluteInclude.c.i"
expected file="expected/Test0014_AbsoluteInclude.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
before
	class= at.jku.weiner.cpreprocess.xtexttests.TestUtils
	method=setUpTest0014
	()
