//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0017_AnotherSimpleDef.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH DEFINE WS ID WS ID NEWLINE
	HASH DEFINE WS ID WS SPECIAL NEWLINE
	HASH DEFINE WS ID WS ID NEWLINE
	HASH DEFINE WS ID WS SPECIAL +*2 NEWLINE
	NEWLINE
	ID WS ID WS SPECIAL WS ID WS SPECIAL WS ID SPECIAL NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(DefineDirective
					id++"A01",
					string++"a01"
				)
			),
			(PreprocessorDirectives
				directive=
				(DefineDirective
					id++"BIG_NUMBER",
					string++"5"
				)
			),
			(PreprocessorDirectives
				directive=
				(DefineDirective
					id++"__INT__",
					string++"int"
				)
			),
			(PreprocessorDirectives
				directive=
				(DefineDirective
					id++"_NUMBER",
					string++"15"
				)
			),
			(NewLineLine),
			(Code
				code++"__INT__ A01 = BIG_NUMBER + _NUMBER;"
			)
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0017_AnotherSimpleDef.c.i"
expected file="expected/Test0017_AnotherSimpleDef.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
