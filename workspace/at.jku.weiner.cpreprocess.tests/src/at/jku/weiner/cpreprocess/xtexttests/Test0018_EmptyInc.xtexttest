//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0018_EmptyInc.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH DEFINE WS ID NEWLINE
	ID WS HASH INCLUDE WS SKW_LESS ID SKW_DOT ID SKW_GREATER NEWLINE
	NEWLINE
	HASH DEFINE WS ID WS WS NEWLINE
	ID WS HASH INCLUDE WS SKW_LESS ID SKW_DOT ID SKW_GREATER NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(DefineDirective
					id++"EMPTY",
					string isNull
				)
			),
			(Code
				code++"EMPTY #include <file.h>"
			),
			(NewLineLine),
			(PreprocessorDirectives
				directive=
				(DefineDirective
					id++"EMPTY2",
					string++""
				)
			),
			(Code
				code++"EMPTY2 #include <file.h>"
			)
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0018_EmptyInc.c.i"
expected file="expected/Test0018_EmptyInc.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
