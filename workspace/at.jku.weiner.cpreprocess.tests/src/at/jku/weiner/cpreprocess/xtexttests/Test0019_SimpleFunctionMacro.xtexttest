//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0019_SimpleFunctionMacro.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH DEFINE WS ID LPAREN RPAREN WS ID LPAREN RPAREN NEWLINE
	NEWLINE
	ID WS ID LPAREN RPAREN SPECIAL NEWLINE
	ID WS ID WS LPAREN WS RPAREN SPECIAL NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(DefineDirective
					id++"halt",
					string++"stop()"
				)
			),
			(NewLineLine),
			(Code
				code++"void halt();"
			),
			(Code
				code++"void halt (	);"
			)
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0019_SimpleFunctionMacro.c.i"
expected file="expected/Test0019_SimpleFunctionMacro.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
