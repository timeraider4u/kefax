//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0023_FunctionMacroOneArgument.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH DEFINE WS ID SKW_LEFTPAREN ID SKW_RIGHTPAREN WS ID WS SKW_PLUS WS DECIMAL_LITERAL NEWLINE
	ID WS ID WS SKW_ASSIGN WS ID SKW_LEFTPAREN DECIMAL_LITERAL SKW_RIGHTPAREN SKW_SEMI NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(DefineFunctionLikeMacro
					id++"inc",
					list=
					(IdentifierList
						id+*"[x]"
					),
					string++"x + 1"
				)
			),
			(Code
				code++"int x = inc(5);"
			)
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0023_FunctionMacroOneArgument.c.i"
expected file="expected/Test0023_FunctionMacroOneArgument.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
