//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0026_FunctionMacroWithString.c"
//----------------------------------------------------------------------
lexer-tokens
	HASH DEFINE WS ID LPAREN ID RPAREN WS ID COMMA WS SPECIAL ID SPECIAL
		COMMA WS SPECIAL ID WS ID WS ID SPECIAL NEWLINE
	ID LPAREN ID RPAREN NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(PreprocessorDirectives
				directive=
				(DefineFunctionLikeMacro
					id++"foo",
					list=
					(IdentifierList
						id+*"[x]"
					),
					string++"x, \\\"x\\\", \\\"abc x def\\\""
				)
			),
			(Code
				code++"foo(bar)"
			)
			
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0026_FunctionMacroWithString.c.i"
expected file="expected/Test0026_FunctionMacroWithString.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
