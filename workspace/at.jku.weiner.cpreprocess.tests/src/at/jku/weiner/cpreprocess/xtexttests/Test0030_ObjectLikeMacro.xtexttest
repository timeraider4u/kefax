//----------------------------------------------------------------------
//----------------------------------------------------------------------
package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0030_ObjectLikeMacro.c"
//----------------------------------------------------------------------
lexer-tokens
	ID WS ID WS SPECIAL WS ID SPECIAL NEWLINE
	HASH DEFINE WS ID WS SPECIAL NEWLINE
	ID WS ID WS SPECIAL WS ID SPECIAL NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		group=
		(GroupOpt
			lines+=
			(Code
				code++"int foo1 = BAR;"
			),
			(PreprocessorDirectives
				directive=
				(DefineDirective
					id++"BAR",
					string++"0"
				)
			),
			(Code
				code++"int foo2 = BAR;"
			)
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0030_ObjectLikeMacro.c.i"
expected file="expected/Test0030_ObjectLikeMacro.c"
//replacePatterns file="res/Patterns.txt"
//----------------------------------------------------------------------
//----------------------------------------------------------------------
