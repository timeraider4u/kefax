<?xml version="1.0" encoding="UTF-8"?>
<project default="feature_export" name="build">
	<target name="feature_export">
		<property name="dest" value="../at.jku.weiner.xtexttest.updatesite" />
		<delete file="${dest}/artifacts.jar"/>
		<delete file="${dest}/content.jar"/>
		<delete>
			<fileset dir="${dest}/features" includes="*.*"/>
		</delete>
		<delete>
			<fileset dir="${dest}/plugins" includes="*.*"/>
		</delete>
		<pde.exportFeatures destination="${dest}" exportSource="false" exportType="directory" features="at.jku.weiner.xtexttest.feature" qualifier="201510120229" useJARFormat="true"/>
	</target>
</project>
