package at.jku.weiner.cpreprocess
language cPreprocess
//----------------------------------------------------------------------
split-lexer=true
source file="res/Test0001_Semicolons.c"
//----------------------------------------------------------------------
lexer-tokens
	MYCODE NEWLINE
	MYCODE +*6 NEWLINE
	NEWLINE
	MYCODE +*3 NEWLINE
//----------------------------------------------------------------------
(Model
	units+=
	(TranslationUnit
		lines+=
		(Code
			code++";"
		),
		(Code
			code++";\\\\;\\\\\\\\;"
		),
		(NewLineLine
			
		),
		(Code
			code++";;;"
		)
	)
)
//----------------------------------------------------------------------
output file="bin/Test0001_Semicolons.c.i"
expected file isSameAsInputFile
//replacePatterns file="res/Patterns.txt"
