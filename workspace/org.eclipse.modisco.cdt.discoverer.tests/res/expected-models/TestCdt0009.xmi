<?xml version="1.0" encoding="ASCII"?>
<cdt:Model xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cdt="http://www.eclipse.org/MoDisco/CDT/0.1.incubation/cdt">
  <statements xsi:type="cdt:Type" name="int"/>
  <statements xsi:type="cdt:Type" name="string"/>
  <units parent="/" name="Test.c" fullPath="${path}/Test.c">
    <statements xsi:type="cdt:PreprocessorIncludeStatement" name="stdio.h" standard="true" path=""/>
    <statements xsi:type="cdt:FunctionDefinition" name="doSomething" parent="//@units.0" returnType="//@statements.0" body="//@units.0/@statements.1/@statements.0">
      <statements xsi:type="cdt:CompoundStatement" parent="//@units.0/@statements.1">
        <statements xsi:type="cdt:Variable" name="a" type="//@statements.0"/>
        <statements xsi:type="cdt:ExpressionStatement">
          <expression xsi:type="cdt:BinaryExpression" operator="assign">
            <operand1 xsi:type="cdt:VariableExpression" variable="//@units.0/@statements.1/@statements.0/@statements.0"/>
            <operand2 xsi:type="cdt:LiteralExpression" value="0" type="//@statements.0"/>
          </expression>
        </statements>
        <statements xsi:type="cdt:NullStatement"/>
        <statements xsi:type="cdt:NullStatement"/>
        <statements xsi:type="cdt:ExpressionStatement">
          <expression xsi:type="cdt:BinaryExpression" operator="assign">
            <operand1 xsi:type="cdt:VariableExpression" variable="//@units.0/@statements.1/@statements.0/@statements.0"/>
            <operand2 xsi:type="cdt:BinaryExpression" operator="plus">
              <operand1 xsi:type="cdt:LiteralExpression" value="1" type="//@statements.0"/>
              <operand2 xsi:type="cdt:LiteralExpression" value="2" type="//@statements.0"/>
            </operand2>
          </expression>
        </statements>
        <statements xsi:type="cdt:ExpressionStatement">
          <expression xsi:type="cdt:FunctionCallExpression" functionName="printf">
            <arguments xsi:type="cdt:LiteralExpression" value="a is %d\n" type="//@statements.1"/>
            <arguments xsi:type="cdt:VariableExpression" variable="//@units.0/@statements.1/@statements.0/@statements.0"/>
          </expression>
        </statements>
        <statements xsi:type="cdt:ReturnStatement">
          <expression xsi:type="cdt:VariableExpression" variable="//@units.0/@statements.1/@statements.0/@statements.0"/>
        </statements>
      </statements>
    </statements>
  </units>
</cdt:Model>
